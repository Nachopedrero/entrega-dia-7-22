class Alumno:
    
    def __init__(self,nombre,nota):
        self.nombre = nombre
        self.nota = nota
        print("la calificacion de ", self.nombre "es " self.nota)

    def calificacion(self.alumno):
        if self.nota>= 5:
            print("el alumno ha aprobado")
        else:
            print("el alumno ha suspendido")         


s = Alumno("Juan",5)        
